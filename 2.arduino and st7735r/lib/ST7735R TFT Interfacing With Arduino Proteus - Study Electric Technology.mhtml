.menu-upper { top: 15px; }

.menu-lower { bottom: 15px; }

.menu-btn-section { padding: 5px 0px; margin: 0px; box-shadow: rgba(0, 0, 0, 0.2) 0px 0px 3px 3px; }

.menu-btn-section img { height: 21px; margin: 3px 14px 0px 0px; }

[dir="rtl"] .menu-btn-section img { margin: 3px 0px 0px 14px; }

.menu-btn-section a { box-sizing: border-box; display: table; padding: 0px 14px; width: 100%; }

.menu-btn-section a div { display: table-cell; vertical-align: middle; }

div.img-container { width: 35px; }

.menu-btn-section a span { display: inline-block; color: rgb(33, 33, 33); font-family: Roboto-Regular, arial, sans-serif; font-size: 14px; margin: 11px 0px; max-width: 224px; }

.survey-confirmation-header span { opacity: 0.7; }

.interstitial-menu { position: absolute; top: 12px; left: 12px; }
